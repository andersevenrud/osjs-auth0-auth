{
  "name": "@osjs/auth0-auth",
  "version": "0.5.1",
  "description": "OS.js v3 Auth0 authentication provider adapter",
  "main": "dist/main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "eslint": "eslint *.js",
    "build": "webpack",
    "watch": "webpack --watch",
    "prepublishOnly": "npm run eslint && rm ./dist/* && NODE_ENV=production npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andersevenrud/osjs-auth0-auth.git"
  },
  "keywords": [
    "osjs"
  ],
  "files": [
    "dist/",
    "server.js",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "express-jwt": "^5.3.1",
    "express-jwt-authz": "^1.0.0",
    "jwks-rsa": "^1.3.0"
  },
  "author": "Anders Evenrud <andersevenrud@gmail.com>",
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/andersevenrud/osjs-auth0-auth/issues"
  },
  "homepage": "https://github.com/andersevenrud/osjs-auth0-auth#readme",
  "devDependencies": {
    "@osjs/event-emitter": "^1.0.4",
    "@babel/core": "^7.1.2",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/runtime": "^7.1.2",
    "babel-loader": "^8.0.4",
    "eslint": "^5.6.1",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    },
    "parserOptions": {
      "sourceType": "module",
      "ecmaVersion": 8
    },
    "globals": {
      "Promise": true,
      "ArrayBuffer": true,
      "File": true,
      "Blob": true,
      "OSJS_VERSION": true
    },
    "extends": "eslint:recommended",
    "rules": {
      "linebreak-style": ["error", "unix"],
      "semi": ["error", "always"],
      "indent": ["error", 2],
      "radix": 1,
      "eol-last": ["error", "always"],
      "consistent-return": 1,
      "space-in-parens": ["error", "never"],
      "space-before-function-paren": ["error", {
          "anonymous": "never",
          "named": "never",
          "asyncArrow": "always"
      }],
      "space-before-blocks": ["error", "always"],
      "space-infix-ops": ["error", {"int32Hint": false}],
      "space-unary-ops": ["error", {"words": true, "nonwords": false}],
      "spaced-comment": ["warn", "always"],
      "prefer-arrow-callback": 1,
      "prefer-spread": 1,
      "prefer-rest-params": 1,
      "template-curly-spacing": ["warn", "never"],
      "no-unused-vars": ["error", {"vars": "all", "args": "none"}],
      "no-useless-constructor": 1,
      "no-var": 2,
      "no-duplicate-imports": 2,
      "no-console": "off",
      "no-extra-semi": 2,
      "no-eval": 2,
      "no-invalid-this": 1,
      "no-new-func": 2,
      "no-return-await": 1,
      "no-self-compare": 2,
      "no-with": 2,
      "no-multi-assign": 1,
      "no-new-object": 2,
      "no-tabs": 1
    }
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-transform-runtime"
    ]
  }
}
