{
  "name": "@osjs/auth0-auth",
  "version": "0.5.1",
  "description": "OS.js v3 Auth0 authentication provider adapter",
  "main": "dist/main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "eslint": "eslint *.js",
    "build": "webpack",
    "watch": "webpack --watch",
    "prepublishOnly": "npm run eslint && rm ./dist/* && NODE_ENV=production npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andersevenrud/osjs-auth0-auth.git"
  },
  "keywords": [
    "osjs"
  ],
  "files": [
    "dist/",
    "server.js",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "@osjs/event-emitter": "^1.0.4",
    "express-jwt": "^5.3.1",
    "express-jwt-authz": "^1.0.0",
    "jwks-rsa": "^1.3.0"
  },
  "author": "Anders Evenrud <andersevenrud@gmail.com>",
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/andersevenrud/osjs-auth0-auth/issues"
  },
  "homepage": "https://github.com/andersevenrud/osjs-auth0-auth#readme",
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/runtime": "^7.1.2",
    "@osjs/eslint-config": "^1.0.2",
    "babel-loader": "^8.0.4",
    "eslint": "^5.6.1",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    },
    "parserOptions": {
      "sourceType": "module",
      "ecmaVersion": 8
    },
    "extends": "@osjs/eslint-config"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-transform-runtime"
    ]
  }
}
